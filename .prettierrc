{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"semi": true,
	"experimentalTernaries": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"quoteProps": "consistent",
	"trailingComma": "es5",
	"singleAttributePerLine": false,
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": false,
	"proseWrap": "always",
	"insertPragma": false,
	"printWidth": 80,
	"requirePragma": false,
	"useTabs": false,
	"embeddedLanguageFormatting": "auto",
	"tabWidth": 2
}
